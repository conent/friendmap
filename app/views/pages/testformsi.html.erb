<%= form_tag("/uploadimage", method: "POST", :multipar => true) do %>
  <%= label_tag(:id, "id") %>
  <%= text_field_tag(:id) %>

    <%= label_tag(:name, "name") %>
  <%= text_field_tag(:name) %>

  <p><label for="upload_file">Select File</label> : 
	<%= file_field ':picture', 'picture' %></p>

  <%= submit_tag("gogogogo") %>
<% end %>